      "@babel/traverse@<7.23.2": ">=7.23.2",
      "vite@>=5.0.0 <5.0.5": ">=5.0.5",
      "vite@>=5.0.0 <=5.0.11": ">=5.0.12",
      "ip@<1.1.9": ">=1.1.9",
