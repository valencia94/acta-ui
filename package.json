{
  "name": "@ctux/vitejs-react-ts-template",
  "version": "3.6.0",
  "homepage": "https://github.com/cTux/vitejs-react-ts-template",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "^22",
    "pnpm": "^9"
  },
  "scripts": {
    "build": "vite build",
    "build:analyze": "vite-bundle-visualizer",
    "dev": "vite",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "tsc --noEmit && eslint .",
    "postinstall": "if [ -z \"$CI\" ]; then check-engine; fi",
    "prepare": "if [ -z \"$CI\" ]; then playwright install chromium && husky; fi",
    "preview": "vite preview",
    "serve": "vite serve",
    "test": "pnpm run test:vitest && pnpm run test:playwright",
    "test:vitest": "vitest --run --passWithNoTests",
    "test:vitest:coverage": "vitest --run --coverage",
    "test:vitest:watch": "vitest",
    "update:major": "pnpm exec npm-check-updates -u --packageManager pnpm",
    "update:minor": "pnpm exec npm-check-updates -u --target minor --packageManager pnpm",
    "update:patch": "pnpm exec npm-check-updates -u --target patch --packageManager pnpm"
  },
  "dependencies": {
    "@emotion/styled": "^11.13.0",
    "@headlessui/react": "^1.7.18",
    "@mui/material": "^6.1.6",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^6.1.6",
    "@mui/styled-engine-sc": "^6.1.6",
    "clsx": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "styled-components": "^6.1.13",
    "zod": "^3.23.8"
  },
  "packageManager": "pnpm@9.15.9",
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^6.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@julr/vite-plugin-validate-env": "^1.1.1",
    "@playwright/test": "^1.48.2",
    "@storybook/addon-actions": "^8.4.2",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-links": "^8.4.2",
    "@storybook/react": "^8.4.2",
    "@storybook/react-vite": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.4",
    "autoprefixer": "^10.4.21",
    "ctux-check-engine": "^1.13.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "gh-pages": "^6.2.0",
    "happy-dom": "^15.11.2",
    "husky": "^9.1.6",
    "node-fetch": "^3.3.2",
    "npm-check-updates": "^17.1.10",
    "playwright": "^1.48.2",
    "postcss": "^8.4.43",
    "prettier": "^3.3.3",
    "storybook": "^8.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.5.4",
    "vite": "^5.4.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.4"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "got@<11.8.5": ">=11.8.5",
      "terser@>=5.0.0 <5.14.2": ">=5.14.2",
      "minimatch@<3.0.5": ">=3.0.5",
