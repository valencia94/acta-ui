{
  "name": "acta-ui",
  "version": "0.1.0",
  "homepage": "https://github.com/valencia94/acta-ui",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20 <23",
    "pnpm": "^9"
  },
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "ci:check": "pnpm exec tsc --noEmit && pnpm exec eslint . --fix",
    "ci:test": "pnpm exec vitest --run --coverage",
    "ci": "pnpm run ci:check && pnpm run ci:test",
    "build": "vite build",
    "build:analyze": "vite-bundle-visualizer",
    "build:storybook": "storybook build -o storybook-static",
    "dev": "vite",
    "dev:api": "sam local start-api",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "tsc --noEmit && eslint .",
    "format": "prettier --write .",
    "setup": "bash setup_playwright_ffmpeg.sh",
    "prepare": "node scripts/prepare.cjs",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "node scripts/run_playwright.cjs",
    "test:e2e:live": "node scripts/run_playwright.cjs tests/e2e.live.spec.ts",
    "test:vitest": "vitest --run --passWithNoTests",
    "test:vitest:coverage": "vitest --run --coverage",
    "test:vitest:watch": "vitest",
    "type-check": "tsc --noEmit",
    "update:major": "pnpm exec npm-check-updates -u --packageManager pnpm",
    "update:minor": "pnpm exec npm-check-updates -u --target minor --packageManager pnpm",
    "update:patch": "pnpm exec npm-check-updates -u --target patch --packageManager pnpm",
    "check:env": "node scripts/check-env.cjs",
    "predeploy": "pnpm run build",
    "deploy": "bash scripts/push-spa-routes.sh"
  },
  "dependencies": {
