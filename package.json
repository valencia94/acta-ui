{
  "name": "acta-ui",
  "version": "0.1.0",
  "homepage": "https://github.com/valencia94/acta-ui",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20 <23",
    "pnpm": "^9"
  },
  "packageManager": "pnpm@9.15.9",

  "scripts": {
    "build": "vite build",
    "build:analyze": "vite-bundle-visualizer",
    "build:storybook": "storybook build -o storybook-static",
    "dev": "vite",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "tsc --noEmit && eslint .",
    "postinstall": "echo \"postinstall: engine check skipped\"",
    "prepare": "if [ -z \"$CI\" ]; then playwright install chromium && husky; fi",
    "preview": "vite preview",
    "test": "pnpm run test:vitest && pnpm run test:playwright",
    "test:playwright": "echo \"(skipped)\"",
    "test:vitest": "vitest --run --passWithNoTests",
    "test:vitest:coverage": "vitest --run --coverage",
    "test:vitest:watch": "vitest",
    "update:major": "pnpm exec npm-check-updates -u --packageManager pnpm",
    "update:minor": "pnpm exec npm-check-updates -u --target minor --packageManager pnpm",
    "update:patch": "pnpm exec npm-check-updates -u --target patch --packageManager pnpm",
    "format": "prettier --write ."
  },

  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@mui/material": "^6.4.11",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^6.4.9",
    "@mui/styled-engine-sc": "^6.4.9",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.18",
    "zod": "^3.25.18"
  },

  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@julr/vite-plugin-validate-env": "^1.3.0",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "node-fetch": "^3.3.2",
    "npm-check-updates": "^17.1.18",
    "playwright": "^1.52.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3",
    "vite": "^5.4.19",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.9"
  },

  "pnpm": {
    "overrides": {
      "@mui/styled-engine": "npm:@mui/styled-engine-sc@^6.0.1",

      "vite@<5.4.19": ">=5.4.19",
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "got@<11.8.5": ">=11.8.5",
      "terser@<5.14.2": ">=5.14.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "json5@<2.2.2": ">=2.2.2",
      "http-cache-semantics@<4.1.1": ">=4.1.1",
      "cacheable-request@<10.2.7": ">=10.2.7",
      "semver@<7.5.2": ">=7.5.2",
      "word-wrap@<1.2.4": ">=1.2.4",
      "get-func-name@<2.0.1": ">=2.0.1",
      "zod@<=3.22.2": ">=3.22.3",
      "postcss@<8.4.31": ">=8.4.31",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "ip@<1.1.9": ">=1.1.9",
      "express@<4.19.2": ">=4.19.2",
      "tar@<6.2.1": ">=6.2.1",
      "ws@<8.17.1": ">=8.17.1",
      "micromatch@<4.0.8": ">=4.0.8",
      "body-parser@<1.20.3": ">=1.20.3",
      "send@<0.19.0": ">=0.19.0",
      "serve-static@<1.16.0": ">=1.16.0",
      "path-to-regexp@<0.1.10": ">=0.1.10",
      "rollup@<4.22.4": ">=4.22.4",
      "cookie@<0.7.0": ">=0.7.0",
      "happy-dom@<15.10.2": ">=15.10.2",
      "cross-spawn@<7.0.5": ">=7.0.5",
      "nanoid@<3.3.8": ">=3.3.8",
      "esbuild@<=0.24.2": ">=0.25.0",
      "vitest@<2.1.9": ">=2.1.9",
      "@babel/runtime@<7.26.10": ">=7.26.10",
      "@babel/helpers@<7.26.10": ">=7.26.10"
    }
  }
}
